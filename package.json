{
  "name": "QMapEditor",
  "version": "2.0.2",
  "main": "app/js/main.js",
  "author": "Quxios",
  "description": "Map Editor",
  "license": "MIT",
  "scripts": {
    "start": "electron . --dev",
    "compile:babel": "babel app -d compiled --copy-files -i node_modules,style.less",
    "compile": "npm run compile:babel",
    "build": "npm run compile && build -w --x64 --ia32",
    "build:w64": "npm run compile && build -w --x64",
    "build:w32": "npm run compile && build -w --ia32"
  },
  "dependencies": {
    "marked": "^0.3.6",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "pixi.js": "^4.5.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-sortable-hoc": "^0.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-electron": "^1.4.15",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "electron": "^1.6.5",
    "electron-builder": "^17.0.1"
  },
  "build": {
    "directories": {
      "app": "compiled"
    },
    "win": {
      "target": "dir",
      "icon": "app/imgs/icon.ico"
    }
  }
}
